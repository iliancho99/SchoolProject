#include <stdio.h>
#include <stdlib.h>

void main(void)
{
    int n,a,max,i;
    scanf("%d",&n);
    i = 2;
    scanf("%d",&a);
    max = a;
    while(i <= n)
    {
        scanf("%d",&a);
        if(a > max)
        {
            max = a;
        }
        i = i + 1;
    }
    printf("Max = %d", max);
}

